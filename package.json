{
  "name": "react-mui-admin",
  "version": "3.2.0",
  "description": "Base-UI is a professionally made React dashboard template with ready-to-use Material UI components.",
  "keywords": [
    "admin-template",
    "ant-design",
    "dashboard-templates",
    "material-ui",
    "mui",
    "react",
    "react-dashboard"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Focela Technologies",
      "email": "opensource@focela.com",
      "homepage": "https://opensource.focela.com"
    }
  ],
  "dependencies": {
    "@ant-design/colors": "^7.0.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.12",
    "@mui/system": "^5.15.12",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.2",
    "stylis-plugin-rtl": "^2.1.1",
    "vite-tsconfig-paths": "^4.3.1",
    "web-vitals": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.23.10",
    "@craco/craco": "^7.1.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/webpack-env": "^1.18.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "craco-alias": "^3.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.2.5",
    "typescript": "5.1.6",
    "vite": "^5.1.5"
  },
  "scripts": {
    "start": "npx husky install & vite",
    "build": "tsc && vite build",
    "build-stage": "env-cmd -f .env vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --cache --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
